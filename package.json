  "main": "pinger.js",
  "scripts": {
    "unit_test": "_mocha test/unit",
    "unit_test_report": "([ -d reports ] || mkdir reports) && (_mocha -R xunit test/unit | grep \"<\" > reports/unit-tests.xml)",
