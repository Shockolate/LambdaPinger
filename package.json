    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "istanbul": "latest",
    "mocha": "latest",
    "should": "latest",
    "sinon": "latest",
    "sinon-chai": "latest",
    "swagger-cli": "^1.0.0-beta.2",
    "swagger-parser": "^3.4.1"
